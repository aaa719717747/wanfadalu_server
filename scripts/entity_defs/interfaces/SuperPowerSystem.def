<root>

	<Properties>

		<MSP><!--最大一次灵力释放量-->
			<Type>			INT32				</Type>
			<Flags>			ALL_CLIENTS			</Flags>
			<Persistent>	true					</Persistent> 
			<Default>		10				       </Default>
		</MSP>

		<MSP_Max><!--最大一次灵力释放量上限-->
			<Type>			INT32				</Type>
			<Flags>			ALL_CLIENTS			</Flags>
			<Persistent>	true					</Persistent> 
			<Default>		100				</Default>
		</MSP_Max>

		<SP><!--灵力总量-->
			<Type>          INT32                </Type>
			<Flags>         ALL_CLIENTS               </Flags>
			<Persistent>    true                 </Persistent>
			<Default>       100                  </Default>
		</SP>

		<SP_Max><!-->灵力总量上限值，会随时间增大<-->
			<Type>          INT32                 </Type>
			<Flags>         ALL_CLIENTS           </Flags>
			<Persistent>    true                 </Persistent>
			<Default>       100                    </Default>
		</SP_Max>

		<talent><!-->天赋，无上限值，天赋越高，单位时间角色 灵力上限值 提升越多<-->
			<Type>          INT32                 </Type>
			<Flags>         ALL_CLIENTS           </Flags>
			<Persistent>    true                 </Persistent>
			<Default>       5                    </Default>
		</talent>

		<level><!-->等级，既取决于SP_Max，也会束缚SP_Max，当 灵力上限值 达到当前等级的最大值时，角色必须晋级，灵力上限值 才会继续随时间提升<-->
			<Type>          INT32           </Type>
			<Flags>         ALL_CLIENTS     </Flags>
			<Persistent>    true              </Persistent>
			<Default>       1                </Default>
		</level>

		<levelName><!-->等级名称，既取决于SP_Max，也会束缚SP_Max，当 灵力上限值 达到当前等级的最大值时，角色必须晋级，灵力上限值 才会继续随时间提升<-->
			<Type>          UNICODE           </Type>
			<Flags>         ALL_CLIENTS     </Flags>
			<Persistent>    true              </Persistent>
			<Default>       1                </Default>
		</levelName>

		<levelPeriod><!-->等级时期，既取决于SP_Max，也会束缚SP_Max，当 灵力上限值 达到当前等阶的最大值时，角色必须进阶，灵力上限值 才会继续随时间提升<-->
			<Type>          INT32           </Type>
			<Flags>         ALL_CLIENTS     </Flags>
			<Persistent>    true              </Persistent>
			<Default>       1                </Default>
		</levelPeriod>

		<levelPeriodName><!-->等级时期名称，既取决于SP_Max，也会束缚SP_Max，当 灵力上限值 达到当前等阶的最大值时，角色必须进阶，灵力上限值 才会继续随时间提升<-->
			<Type>          UNICODE           </Type>
			<Flags>         ALL_CLIENTS     </Flags>
			<Persistent>    true              </Persistent>
			<Default>       1                </Default>
		</levelPeriodName>

	</Properties>

	<ClientMethods>

	</ClientMethods>

	<BaseMethods>

	</BaseMethods>

	<CellMethods>

	</CellMethods>

</root>